    "jstimezonedetect": "",
    "ui-select": "git+https://github.com/angular-ui/ui-select.git#v0.19.8"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.24",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "expect": "^23.5.0",
    "file-loader": "^1.1.11",
    "gulp": "^3.9.1",
    "gulp-cli": "^2.0.1",
    "gulp-concat": "^2.6.1",
    "gulp-jshint": "^2.1.0",
    "gulp-stylelint": "^7.0.0",
    "gulp-svg-sprite": "1.4.0",
    "jest": "^23.5.0",
